for r in range(300):
  col = random.choice([19,20])
  for s in range(19):
    k = random.choice([1,-1])
    col = col + k
  f = 0
  for d in range(40):
    x = 39 - d
    if A[x][col] == 0:
      if f == 0:
        A[x][col] = 1
        f = 1
